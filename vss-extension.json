{
  "manifestVersion": 1,
  "id": "processtemplate",
  "name": "VSTS Process Tools",
  "version": "#{GitVersion.Major}#.#{GitVersion.Minor}#.#{GitVersion.Patch}#",
  "public": false,
  "publisher": "nkdAgility",
  "galleryFlags": [
    "Public",
    "Free"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Incudes tasks to help you manage your Processes (was Process Templates) in VSTS. Supports both XML Process and the new Inherited Process.",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "release",
    "build",
    "process",
    "process template",
    "xml process",
    "template",
    "upload",
    "import",
    "export",
    "migrate"
  ],
  "links": {
    "home": {
      "uri": "https://nkdagility.visualstudio.com/vsts-processtemplate-task/"
    },
    "getstarted": {
      "uri": "https://nkdagility.visualstudio.com/vsts-processtemplate-task/_wiki/wikis/VSTS-Sync-Migrator-Docs?pagePath=%2Fgetting%20started"
    },
    "learn": {
      "uri": "https://nkdagility.visualstudio.com/vsts-processtemplate-task/_wiki/wikis/vsts-processtemplate-task"
    },
    "support": {
      "uri": "https://nkdagility.visualstudio.com/vsts-processtemplate-task/_workitems"
    },
    "repository": {
      "uri": "https://github.com/nkdAgility/vsts-process-template-manager"
    },
    "issues": {
      "uri": "https://github.com/nkdAgility/vsts-process-template-manager/issues"
    }
  },
  "CustomerQnASupport": {
    "enableqna": "true",
    "url": "https://github.com/nkdAgility/vsts-process-template-manager/issues"
  },
  "repository": {
    "type": "git",
    "uri": "https://nkdagility.visualstudio.com/DefaultCollection/vsts-processtemplate-task/_git/vsts-sync-migrator"
  },
  "icons": {
    "default": "images/extension-icon.png"
  },
  "branding": {
    "color": "#1C3272",
    "theme": "dark"
  },
  "screenshots": [
    {
      "path": "images/screenshot-01.png"
    },
    {
      "path": "images/screenshot-02.png"
    },
    {
      "path": "images/screenshot-03.png"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.txt"
    }
  },
  "files": [
    {
      "path": "processtemplatetask"
    }
  ],
  "contributions": [
    {
      "id": "processtemplate-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "processtemplatetask"
      }
    },
    {
      "id": "vstsProcess-endpoint",
      "description": "Service Endpoint for XML Process Uploads",
      "type": "ms.vss-endpoint.service-endpoint-type",
      "url": "https://myaccount.visualstudio.com",
      "targets": [ "ms.vss-endpoint.endpoint-types" ],
      "properties": {
        "name": "VstsXmlProcessEndpoint",
        "displayName": "VSTS XML Process Endpoint",
        "authenticationSchemes": [
          {
            "type": "ms.vss-endpoint.endpoint-auth-scheme-token"
          }
        ],
        "helpMarkDown": "<a href=\"http://vsts-process-template-management.readthedocs.io\" target=\"_blank\"><b>Learn More</b></a>"
      }
    }
  ]
}