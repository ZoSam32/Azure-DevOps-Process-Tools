{
    "manifestVersion": 1,
    "id": "processtemplate#{GitVersion.PreReleaseLabel}#",
    "name": "Process Template Manager #{GitVersion.PreReleaseLabel}#",
    "version": "#{GitVersion.Major}#.#{GitVersion.Minor}#.#{GitVersion.Patch}#.#{GitVersion.COMMITSSINCEVERSIONSOURCE}#",
    "public": false,
    "publisher": "nkdAgility",
    "galleryFlags": [
        "#{Extension.galleryFlags}#"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Incudes tasks to help you manage your Process Templates in VSTS.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "release",
        "build",
        "token",
        "replace",
        "variable",
        "substitution"
    ],
    "links": {
        "learn": {
            "uri": "https://github.com/nkdAgility/vsts-process-template-task#readme"
        },
        "support": {
            "uri": "https://github.com/nkdAgility/vsts-process-template-task/issues"  
        }
    },
    "icons": {
        "default": "images/extension-icon.png"
    },
    "branding": {
        "color": "#1C3272",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "images/screenshot-01.png"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "files": [
        {
            "path": "processtemplate-task"
        }
    ],
    "contributions": [
        {
            "id": "processtemplate-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "processtemplate-task"
            }
        }
    ]
}